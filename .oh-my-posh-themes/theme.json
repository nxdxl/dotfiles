{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "shell",
            "style": "plain",
            "foreground": "#cba6f7",
            "properties": {
              "mapped_shell_names": {
                "pwsh": "Shell",
                "powershell": "Shell",
                "cmd": "Cmd",
                "bash": "Bash",
				"zsh": "zsh"
              }
            },
            "template": "┌\ue795 {{ .Name }}"
          },
          {
            "foreground": "#cba6f7",
            "foreground_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#cba6f7{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#cba6f7{{ end }}",
              "{{ if gt .Ahead 0 }}#cba6f7{{ end }}",
              "{{ if gt .Behind 0 }}#cba6f7{{ end }}"
            ],
            "properties": {
              "branch_max_length": 25,
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "plain",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "type": "status",
            "style": "plain",
            "foreground": "#a6e3a1",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#f38ba8{{ end }}"
            ],
            "template": " x{{ reason .Code }}"
          },
          {
            "foreground": "#a6e3a1",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#f38ba8{{ end }}"
            ],
            "properties": {
              "style": "roundrock",
              "always_enabled": true
            },
            "style": "diamond",
            "template": " {{ .FormattedMs }} ",
            "type": "executiontime"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#74c7ec",
            "properties": {
              "style": "full"
            },
            "style": "plain",
            "template": "├",
            "type": "path"
          },
          {
            "foreground": "#74c7ec",
            "properties": {
              "style": "full"
            },
            "style": "plain",
            "template": "\ue5ff {{ .Path }}",
            "type": "path"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#74c7ec",
            "style": "plain",
            "template": "\u0021",
            "type": "root"
          },
          {
            "foreground": "#74c7ec",
            "style": "plain",
            "template": "└\u276f",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
